node {
    def app
    stage('Clone repository') {
        git 'https://github.com/MinYeongPark/stop_bang.git'
    }
    stage('Build Image') {
        app = docker.build('minyeongpark5/stopbang')
    }
    stage('Push Image') {
        docker.withRegistry('https://registry.hub.docker.com', 'minyeongdockerhub') {
            app.push('${env.BUILD_NUMBER}')
            app.push('latest')
        }
    }
}